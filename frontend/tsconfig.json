{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
